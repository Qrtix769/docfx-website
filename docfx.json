{
  "metadata": [
    {
      "src": [
        {
          "src": ".",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api",
      "allowCompilationErrors": true,
      "outputFormat": "markdown"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Home",
      "_appTitle": "HCI LLenado-Vaciado",
      "_enableSearch": true,
      "_lang": "es",
      "_appFaviconPath": "images/favicon.png",
      "_appLogoPath": "images/logo.png",
      "_appFooter": "<span>Copyright © 2024 Llenado vaciado - <strong>HabanaClub Internacional</strong> | Desarrollado por <strong>Avangenio ®</strong></span></br>",
      "pdf": true
    }
  }
}